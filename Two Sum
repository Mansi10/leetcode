class Solution {
    public int[] twoSum(int[] nums, int target) {
        int length= nums.length;
        int[] index = new int[2];
        for(int i=0; i<length; i++){
            for(int j=i+1; j<length; j++){
             if(nums[j] == target - nums[i]){
                 index[0]=i;
                 index[1]=j;
             }
            }
        }
        return index;
    }
}

